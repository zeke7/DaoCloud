<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 1、classPathEntry:引入数据库的JDBC驱动的jar包地址 (**此处写自己本地的jar包路径**)-->
    <!--
    1.在左侧project边栏的External Libraries中找到mysql的驱动
    2.右键选择copy path
    -->
    <classPathEntry location="C:\Users\hf\.m2\repository\mysql\mysql-connector-java\8.0.23\mysql-connector-java-8.0.23.jar" />

    <context id="curtain" targetRuntime="MyBatis3">

        <commentGenerator>
            <!-- 2、设置是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <!--3、配置Mysql数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/daocloud?useSSL=false&amp;autoReconnect=true&amp;failOverReadOnly=false"
                        userId="root"
                        password="ni12wo34">
        </jdbcConnection>

        <!--4、配置Oracle数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <!-- <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
             connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:yycg"
             userId="yycg"
             password="yycg">
             </jdbcConnection> -->

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer true，把JDBC DECIMAL 和 NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--5、设置pojo生成位置
          targetProject:设置生成PO类生成位置的工程名称
          targetPackage="com.it.pojo":PO生成的包位置
        -->
        <javaModelGenerator targetPackage="com.fzu.gcxl.daocloud.domain"
                            targetProject="./src/main/java">
            <!-- 1）enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true" />
            <!-- 2）设置从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />

        </javaModelGenerator>
