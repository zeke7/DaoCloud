<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fzu.gcxl.daocloud.mapper.AccountMapper">
    <insert id="addAccount" parameterType="Account">
        insert into account (login_verifycode,login_phone, login_password)
        values (#{loginVerifyCode},#{loginPhone},#{loginPassword})
    </insert>
    <select id="qAList" resultType="Account">
        select * from daocloud.account ;
    </select>

    <select id="queryAccountByPhone" resultType="Account">
        select * from daocloud.account where login_phone = '18039078699';
    </select>

</mapper>