
.qrcode-body {
    width: 100%;
}
.qrcode-body .qrcode {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}
canvas{
    height: 400rpx;
    width: 400rpx;
}
.banner{
  	width: 100% ;
  	background-color: #FFFFFF;
  	-webkit-border-radius: 60rpx 60rpx 0 0;
  	        border-radius: 60rpx 60rpx 0 0;
  	margin-top: -60rpx;
}
.banner ._dl{
  	margin-top: -80rpx;
}
.banner ._dl ._dt{
  	text-align: center;
}
.banner ._dl ._dt image{
  	width: 160rpx;
  	height: 160rpx;
  	-webkit-border-radius:50% ;
  	        border-radius:50% ;
}
.banner ._dl ._dd{
  	text-align: center;
}
.top{
  	width: 100%;
  	height:400rpx;
  	background-size:100% ;
  	background-position:center center;
}
.sharbuttn{
  	display: -webkit-box;
  	display: -webkit-flex;
  	display: flex;
  	-webkit-box-pack: center;
  	-webkit-justify-content: center;
  	        justify-content: center;
}
.btn{
  	width: 260rpx;
  	height: 60rpx;
  	line-height: 60rpx;
  	margin: 0 auto;
  	margin-top: 60rpx;
  	-webkit-border-radius: 40rpx;
  	        border-radius: 40rpx;
  	border: 0;
  	font-size: 26rpx;
  	outline: 0;
  	background: #33b17b;
  	color: #FFFFFF;
  	text-align: center;
}
.content{
  	width: 100%;
  	background-color: #ffffff;
}
.box{
  	width: 100%;
  	height: 450rpx;  	
  	text-align: center;
}
.se{
  	margin-top:20rpx;
  	width:450rpx;
  	height:100%;
}

